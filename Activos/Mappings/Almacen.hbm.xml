<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <joined-subclass name="Activos.Modelo.Almacen,Activos" extends="BaseBO">
    <key column="Id" />
    <many-to-one name="rubro" column="rubroId"/>
    <list name="activos" cascade="all-delete-orphan">
      <key column="almacenId"/>
      <index column="activoId"/>
      <one-to-many class="activos"/>
    </list>
  </joined-subclass>
</hibernate-mapping>


